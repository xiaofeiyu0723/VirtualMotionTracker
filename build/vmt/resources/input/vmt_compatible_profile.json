    "/input/finger/pinky" : { 
        "type" : "trigger",
        "binding_image_point" : [ 0, 0 ],
        "order" : 1
    },
    "/input/skeleton/left" : { 
        "type" : "skeleton",
        "skeleton" : "/skeleton/hand/left",
        "side" : "left",
        "binding_image_point" : [ 0, 0 ]
