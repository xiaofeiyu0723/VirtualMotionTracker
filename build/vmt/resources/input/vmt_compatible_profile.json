  {
    "image": "{vmt}/input/binding_image_light.png"
  },
  "input_bindingui_right" :
  {
    "image": "{vmt}/input/binding_image_light.png"
  },
  "input_source" :
  {
    "/input/system" : { 
        "type" : "button",
        "click" : true,
        "touch" : true,
        "binding_image_point" : [ 0, 0 ],
        "order" : 18
    },
    "/input/a" : { 
        "type" : "button",
        "click" : true,
        "touch" : true,
        "binding_image_point" : [ 0, 0 ],
        "order" : 18
    },
    "/input/b" : { 
        "type" : "button",
        "click" : true,
        "touch" : true,
        "binding_image_point" : [ 0, 0 ],
        "order" : 18
